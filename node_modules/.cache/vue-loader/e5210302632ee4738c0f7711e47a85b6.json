Component({\n  props: {\n    jobs: {\n      type: Array as PropType<Job[]>,\n      required: true\n    },\n    order: {\n      type: String as PropType<OrderTerm>,\n      required: true\n    }\n  },\n  setup(props) {\n    const orderedJobs = computed(() => {\n      return [...props.jobs].sort((a: Job, b: Job) => {\n        return a[props.order] > b[props.order] ? 1 : -1\n      })\n    })\n\n    return { orderedJobs }\n  },\n})\n</script>\n\n<style scoped>\n  .job-list {\n    max-width: 960px;\n    margin: 40px auto;\n  }\n  .job-list ul {\n    padding: 0\n  }\n  .job-list li {\n    list-style-type: none;\n    background: white;\n    padding: 16px;\n    margin: 16px 0;\n    border-radius: 4px;\n  }\n  .job-list h2 {\n    margin: 0 0 10px;\n    text-transform: capitalize;\n  }\n  .salary {\n    display: flex;\n  }\n  .salary img {\n    width: 30px;\n  }\n  .salary p {\n    color: #17bf66;\n    font-weight: bold;\n    margin: 10px 4px;\n  }\n  .list-move {\n    transition: all 1s;\n  }\n</style>"]}]}